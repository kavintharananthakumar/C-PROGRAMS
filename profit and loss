#include <stdio.h>

int main() {
    int sp, cp;

    printf("Enter sp, cp: ");
    scanf("%d %d", &sp, &cp);
    if (cp>sp)
    {
         int loss=cp-sp;
        printf("loss of:%d",loss);
        
    }
    else if (cp<sp)
    {
        int profit=sp-cp;
        printf("profit of:%d",profit);
        
    }
    else
    {
        printf("no profit or loss");
    }
    
    return 0;
}
