#include<stdio.h>
#include<math.h>
int main()
{
    int n,i,rem,sum=0;
    scanf("%d",&n);
    if(n>=0 && n<=11111111)
    {
      for(i=0;n!=0;i++) 
      {
       rem=n%10; 
       sum+=rem*pow(2,i);
       n/=10;
      }
      printf("Decimal: %d",sum);
    }
    else{
        printf("binary number out of range");
    }
    return 0;
}
