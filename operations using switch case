#include<stdio.h>
int main()
{
    int num,choice;
    printf("enter a number and a choice between 1 and 4");
    scanf("%d%d",&num,&choice);
    switch (choice)
    {
    case 1:
        {
            (num%2==0)?printf("even"):printf("odd");
            break;
        }
    case 2:
        {
            (num>=0)?printf("postive"):printf("negative");
             break;
        }
    case 3:
        {
          (num%10>=5)?printf("%d",(num/10)*10+10):printf("d",(num/10)*10);
          break;
        }
    case 4:
        {
            printf("thank you");
            break;
        }
    default:
        {
            printf("invalid choice");
        }
    }
return 0;    
}
