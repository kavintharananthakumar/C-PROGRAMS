#include<stdio.h>
int main()
{
    float temp,C;
    printf("enter temperature ");
    scanf("%f",&temp);
    if (temp>=80)
    {
        float F=temp;
         C=(F-32)*(5.0/9.0);
    }
    else
    {
         C=temp;
    }
    if (C<=10)
    {
        printf("the temperature is cold");
    }
    else if (C>10 && C<=25)
    {
        printf("the temperature is warm");
    }
  
  else
    {
        printf("the temperature is hot");
    }
    return 0;
}
